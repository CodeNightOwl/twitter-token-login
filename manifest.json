        "persistent": true,
        "scripts": [
            "popup/script.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/logo.png",
        "default_popup": "popup/index.html"
    },
    "description": "用token快速登录推特，owl-2024.",
    "icons": {
        "128": "icons/logo.png"
    },
    "manifest_version": 2,
    "name": "推特 Token 登录",
    "permissions": [
        "http://*.twitter.com/*",
        "https://*.twitter.com/*",
        "http://*.x.com/*",
        "https://*.x.com/*"
    ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.0.1",
    "key": ""
}